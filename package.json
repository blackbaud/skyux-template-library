{
  "name": "@blackbaud/my-library",
  "version": "0.0.1",
  "description": "Single-page-application for skyux-spa-test-stache2",
  "main": "bundles/main.umd.js",
  "module": "index.ts",
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "skyux": "skyux",
    "clean": "rimraf dist .srctmp && npm run clean:ngc",
    "clean:build": "npm run clean:ngc && rimraf .srctmp",
    "clean:ngc": "rimraf **/*.ngsummary.json **/*.ngfactory.ts **/*.ngstyle.ts",
    "prepare:ts": "npm run clean && node ./scripts/stage-ts.js",
    "prepare:package": "node ./scripts/prepare-package.js",
    "transpile": "webpack --config ./config/webpack/webpack.bundle.config.js --progress",
    "build": "npm run clean && npm run prepare:ts && npm run transpile && npm run minify && npm run prepare:package && npm run clean:ngc",
    "minify": "uglifyjs dist/bundles/my-library.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/my-library.umd.min.js",
    "test": "skyux test"
  },
  "keywords": [],
  "author": "Blackbaud",
  "license": "ISC",
  "dependencies": {
    "@blackbaud/skyux": "2.0.0-beta.29"
  },
  "devDependencies": {
    "@blackbaud/skyux-builder": "1.0.0-beta.28",
    "codelyzer": "2.0.0-beta.1",
    "fs-extra": "3.0.1",
    "glob": "7.1.1",
    "ngc-webpack": "2.0.0",
    "node-sass": "4.5.3",
    "rimraf": "2.6.1",
    "uglify-js": "3.0.7",
    "webpack": "2.5.1",
    "typescript": "2.0.6"
  }
}
